    "logo": "https://telegra.ph/file/1c41ded2dd871eb36bd7e.png",
    "keywords": [
        "py-tgcalls",
        "telegram bot",
        "video stream",
        "live stream",
        "music stream",
        "mongodb",
        "pyrogram"
    ],
    "website": "https://t.me/levinachannel",
    "repository": "https://github.com/levina-lab/video-stream",
    "success_url": "https://t.me/dlwrml",
